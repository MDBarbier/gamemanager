@model GameEntry
@{
    ViewData["Title"] = "Edit game";
}

<h1>Edit game</h1>


<form asp-action="Save" method="post">
    <div>
        <label asp-for="Id"></label>
        <input asp-for="Id" readonly />
    </div>
    <div>
        <label asp-for="Name"></label>
        <input asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <div>
        <label asp-for="Genre"></label>
        <input asp-for="Genre" />
        <span asp-validation-for="Genre"></span>
    </div>
    <div>
        <label asp-for="Price"></label>
        <input asp-for="Price" />
        <span asp-validation-for="Price"></span>
    </div>
    <div>
        <label asp-for="Owned"></label>
        <input asp-for="Owned" />
        <span asp-validation-for="Owned"></span>
    </div>
    <div>
        <label asp-for="Notes"></label>
        <input asp-for="Notes" />
        <span asp-validation-for="Notes"></span>
    </div>
    <div>
        <label asp-for="Rating"></label>
        <input asp-for="Rating" />
        <span asp-validation-for="Rating"></span>
    </div>
    <div>
        <label asp-for="Ranking"></label>
        <input asp-for="Ranking" />
        <span asp-validation-for="Ranking"></span>
    </div>
    <div>
        <label asp-for="Store"></label>
        <input asp-for="Store" />
        <span asp-validation-for="Store"></span>
    </div>
    @Html.AntiForgeryToken()
    <div>
        <input type="submit" value="Save" />
    </div>
</form>
